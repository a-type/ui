{
	"name": "@a-type/ui",
	"version": "5.0.13",
	"type": "module",
	"repository": {
		"url": "https://github.com/a-type/ui"
	},
	"files": [
		"/dist",
		"/src"
	],
	"sideEffects": [
		".*\\.css$"
	],
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"node": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./uno-preset": {
			"types": "./dist/esm/uno/uno.preset.d.ts",
			"import": "./dist/esm/uno/uno.preset.js",
			"node": "./dist/esm/uno/uno.preset.js",
			"require": "./dist/cjs/uno/uno.preset.js",
			"default": "./dist/cjs/uno/uno.preset.js"
		},
		"./main.css": {
			"import": "./dist/css/main.css",
			"node": "./dist/css/main.css",
			"require": "./dist/css/main.css",
			"default": "./dist/css/main.css"
		}
	},
	"scripts": {
		"build:transpile": "tsc",
		"build:prefix": "node ./scripts/addUnoInclude.js",
		"build:process": "unocss --write-transformed -o ./dist/css/main.css -m -c uno.config.js \"./dist/**/*.js\"",
		"build": "pnpm run build:transpile && pnpm run build:prefix && pnpm run build:process",
		"prepublish": "pnpm run build",
		"icons": "node ./scripts/generateIcons.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"up-radix": "pnpm update \"@radix-ui/*\" --latest",
		"bump": "pnpm run typecheck && bumpp",
		"test": "vitest",
		"test:unit": "vitest --project=unit",
		"test:snapshots": "pnpm build && vitest --project=snapshots",
		"typecheck": "tsc --noEmit",
		"test:browser": "vitest --config=vitest.browser.config.ts",
		"lint": "eslint **/*.{ts,tsx} --fix",
		"prepare": "husky"
	},
	"peerDependencies": {
		"react": "^19",
		"react-dom": "^19",
		"unocss": "^66"
	},
	"dependencies": {
		"@a-type/utils": "^1.1.4",
		"@base-ui/react": "^1.0.0",
		"@emotion/is-prop-valid": "^1.4.0",
		"@unocss/preset-mini": "66.5.4",
		"@unocss/preset-wind3": "66.5.4",
		"@use-gesture/react": "^10.3.1",
		"browser-image-resizer": "^2.4.1",
		"calendar-blocks": "^0.4.0",
		"clsx": "^2.1.1",
		"csstype": "^3.1.3",
		"date-fns": "^4.1.0",
		"formik": "^2.4.6",
		"frimousse": "^0.2.0",
		"motion": "^12.23.6",
		"pluralize": "^8.0.0",
		"react-hot-toast": "^2.6.0",
		"react-is": "^19.1.0",
		"unocss-preset-completion": "^0.3.0",
		"valtio": "^2.1.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@storybook/addon-essentials": "8.4.7",
		"@storybook/addon-interactions": "8.4.7",
		"@storybook/addon-links": "8.4.7",
		"@storybook/blocks": "8.4.7",
		"@storybook/react": "8.4.7",
		"@storybook/react-vite": "8.4.7",
		"@storybook/test": "8.4.7",
		"@testing-library/react": "^16.3.2",
		"@types/dom-chromium-installation-events": "^101.0.4",
		"@types/pluralize": "^0.0.33",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@types/react-is": "^19.0.0",
		"@types/w3c-image-capture": "^1.0.10",
		"@types/web-app-manifest": "^1.0.9",
		"@unocss/cli": "66.5.4",
		"@unocss/core": "66.5.4",
		"@unocss/eslint-config": "^66.6.0",
		"@unocss/transformer-variant-group": "66.5.4",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/ui": "4.0.18",
		"axios": "^1.5.0",
		"bumpp": "^10.3.1",
		"change-case": "^4.1.2",
		"dotenv": "^16.3.1",
		"eslint": "^9.39.2",
		"eslint-plugin-react": "^7.37.5",
		"fs-extra": "^11.1.1",
		"globals": "^17.0.0",
		"husky": "^9.1.7",
		"jiti": "^2.6.1",
		"lint-staged": "^16.2.7",
		"prettier": "^3.0.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"storybook": "8.4.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0",
		"unocss": "^66.5.4",
		"vitest": "^4.0.18",
		"vitest-browser-react": "^2.0.4"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"prettier --write --cache",
			"eslint --fix --cache"
		]
	}
}
