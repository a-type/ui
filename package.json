{
	"name": "@a-type/ui",
	"version": "1.7.11",
	"type": "module",
	"files": [
		"/dist",
		"/src"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"node": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./uno-preset": {
			"types": "./dist/esm/uno.preset.d.ts",
			"import": "./dist/esm/uno.preset.js",
			"node": "./dist/esm/uno.preset.js",
			"require": "./dist/cjs/uno.preset.js",
			"default": "./dist/cjs/uno.preset.js"
		},
		"./main.css": {
			"import": "./dist/css/main.css",
			"node": "./dist/css/main.css",
			"require": "./dist/css/main.css",
			"default": "./dist/css/main.css"
		}
	},
	"scripts": {
		"build:transpile": "tsc && tsc -P tsconfig-cjs.json",
		"build:prefix": "node ./scripts/addUnoInclude.js",
		"build:process": "unocss \"./dist/**/*.js\" -o ./dist/css/main.css --write-transformed -m -c uno.config.js",
		"build": "pnpm run build:transpile && pnpm run build:prefix && pnpm run build:process",
		"ci:version": "pnpm changeset version",
		"ci:publish": "pnpm changeset publish --access=public",
		"prepublish": "pnpm run build",
		"icons": "node ./scripts/generateIcons.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"up-radix": "pnpm update \"@radix-ui/*\" --latest"
	},
	"peerDependencies": {
		"react": "^19",
		"react-dom": "^19",
		"unocss": "^66"
	},
	"dependencies": {
		"@a-type/utils": "^1.1.4",
		"@radix-ui/react-accordion": "^1.2.2",
		"@radix-ui/react-checkbox": "^1.1.3",
		"@radix-ui/react-collapsible": "^1.1.2",
		"@radix-ui/react-context-menu": "^2.2.3",
		"@radix-ui/react-dialog": "^1.1.3",
		"@radix-ui/react-dropdown-menu": "^2.1.3",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-menu": "^2.1.3",
		"@radix-ui/react-popover": "^1.1.3",
		"@radix-ui/react-progress": "^1.1.1",
		"@radix-ui/react-radio-group": "^1.2.2",
		"@radix-ui/react-scroll-area": "^1.2.2",
		"@radix-ui/react-select": "^2.1.3",
		"@radix-ui/react-slider": "^1.2.2",
		"@radix-ui/react-slot": "^1.1.1",
		"@radix-ui/react-switch": "^1.1.2",
		"@radix-ui/react-tabs": "^1.1.2",
		"@radix-ui/react-toast": "^1.2.3",
		"@radix-ui/react-toggle-group": "^1.1.1",
		"@radix-ui/react-tooltip": "^1.1.5",
		"@react-spring/web": "^9.7.5",
		"@unocss/preset-mini": "66.0.0",
		"@unocss/preset-wind3": "66.0.0",
		"@use-gesture/react": "^10.3.1",
		"browser-image-resizer": "^2.4.1",
		"calendar-blocks": "^0.3.1",
		"clsx": "^2.1.1",
		"color-interpolate": "^1.0.5",
		"color-parse": "^2.0.2",
		"color-space": "^2.0.1",
		"csstype": "^3.1.3",
		"date-fns": "^4.1.0",
		"formik": "^2.4.6",
		"pluralize": "^8.0.0",
		"react-hot-toast": "^2.4.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.10",
		"@storybook/addon-essentials": "8.4.7",
		"@storybook/addon-interactions": "8.4.7",
		"@storybook/addon-links": "8.4.7",
		"@storybook/blocks": "8.4.7",
		"@storybook/react": "8.4.7",
		"@storybook/react-vite": "8.4.7",
		"@storybook/test": "8.4.7",
		"@types/pluralize": "^0.0.33",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@types/w3c-image-capture": "^1.0.10",
		"@unocss/cli": "66.0.0",
		"@unocss/core": "66.0.0",
		"@unocss/transformer-variant-group": "66.0.0",
		"axios": "^1.5.0",
		"change-case": "^4.1.2",
		"dotenv": "^16.3.1",
		"fs-extra": "^11.1.1",
		"prettier": "^3.0.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"storybook": "8.4.7",
		"typescript": "^5.7.2",
		"unocss": "66.0.0"
	}
}
